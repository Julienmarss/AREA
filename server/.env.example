# Server Configuration
PORT=8080
NODE_ENV=development
FRONTEND_URL=http://localhost:8081

# GitHub OAuth2
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_REDIRECT_URI=http://localhost:8080/api/v1/services/github/oauth/callback
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# Discord Bot
DISCORD_BOT_TOKEN=your_discord_bot_token
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret
DISCORD_REDIRECT_URI=http://localhost:8080/api/v1/services/discord/oauth/callback

# Spotify OAuth2
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_REDIRECT_URI=http://localhost:8080/api/v1/auth/spotify/callback

# Notion OAuth2
NOTION_CLIENT_ID=your_notion_client_id
NOTION_CLIENT_SECRET=your_notion_client_secret
NOTION_REDIRECT_URI=http://localhost:8080/api/v1/auth/notion/callback

# Database (if used)
DATABASE_URL=postgresql://user:password@localhost:5432/area

# Redis (if used)
REDIS_URL=redis://localhost:6379
